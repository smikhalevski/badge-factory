{
  "name": "badge-factory",
  "version": "0.0.1",
  "description": "Tiny web service for generating badges from URL",
  "author": "Savva Mikhalevski",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smikhalevski/badge-factory.git"
  },
  "homepage": "https://github.com/smikhalevski/badge-factory",
  "main": "target/out/index.js",
  "files": [
    "src/main/",
    "target/out/",
    ".babelrc"
  ],
  "keywords": [
    "badge",
    "shield"
  ],
  "engines": {
    "node": "7.3.0"
  },
  "scripts": {
    "postinstall": "npm run build",
    "clean": "rm -rf target",
    "build": "npm run clean; webpack -p",
    "watch": "npm run clean; webpack --watch --progress --config webpack.watch.config.js",
    "start": "pm2 start target/out/server.js -i 1 --name badge-factory",
    "logs": "pm2 logs badge-factory",
    "test": "jest"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-traverse": "",
    "babel-generator": "",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "webpack": "beta",
    "babel-loader": "^6.2.10",
    "hson-loader": "",
    "file-loader": "",
    "express": "^4.14.0",
    "pdfkit": "",
    "svgo": "",
    "request-promise": "",
    "tinycolor2": "",
    "safe-eval": "",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "pm2": "^2.4.0",
    "jest": "^18.1.0",
    "babel-jest": "^18.0.0"
  }
}

    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "webpack": "beta",
    "babel-loader": "^6.2.10",
    "hson-loader": "",
    "express": "^4.14.0",
    "pdfkit": "",
    "svgo": ""
  },
  "devDependencies": {
    "jest": "^18.1.0",
    "babel-jest": "^18.0.0"
  }
